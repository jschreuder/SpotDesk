{
    "name": "jschreuder/spot-desk",
    "description": "Email based helpdesk ticket software",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "j.schreuder@mijnpraktijk.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "ext-json": "*",
        "jschreuder/middle": "^1.0",
        "robmorgan/phinx": "^0.10",
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.23",
        "zendframework/zend-diactoros": "^2.0",
        "zendframework/zend-httphandlerrunner": "^1.0",
        "symfony/routing": "^4.1",
        "symfony/console": "^4.1",
        "ramsey/uuid": "^3.8",
        "lcobucci/jwt": "^3.2",
        "php-imap/php-imap": "^2.0",
        "swiftmailer/swiftmailer": "^5.4",
        "particle/filter": "^1.4",
        "particle/validator": "^2.2",
        "twig/twig": "^2.3",
        "zendframework/zend-permissions-rbac": "^2.5"
    },
    "require-dev": {
        "ext-imap": "*",
        "phpspec/phpspec": "^4.0",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "fzaninotto/faker": "^1.6",
        "leanphp/phpspec-code-coverage": "^4.0"
    },
    "suggest": {
        "ext-imap": "For reading e-mails into tickets"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "jschreuder\\SpotDesk\\": "src"
        }
    }
}
