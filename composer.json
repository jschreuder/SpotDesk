{
    "name": "jschreuder/spot-desk",
    "description": "Email based helpdesk ticket software",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "j.schreuder@mijnpraktijk.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "ext-json": "*",
        "jschreuder/middle": "^2.0",
        "jschreuder/middle-di": "dev-master",
        "laminas/laminas-diactoros": "^2.24",
        "laminas/laminas-httphandlerrunner": "^2.5",
        "monolog/monolog": "^3.2",
        "robmorgan/phinx": "^0.13",
        "symfony/routing": "^6.0",
        "symfony/console": "^6.0",
        "ramsey/uuid": "^4.7",
        "lcobucci/jwt": "^4.3",
        "symfony/mailer": "^6.0",
        "laminas/laminas-filter": "^2.31",
        "laminas/laminas-validator": "^2.30",
        "twig/twig": "^3.5",
        "laminas/laminas-permissions-rbac": "^3.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2",
        "phploc/phploc": "^7.0",
        "phpmd/phpmd": "^2.13",
        "fakerphp/faker": "^1.21",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1",
        "webklex/php-imap": "^5.0"
    },
    "suggest": {
        "webklex/php-imap": "For reading e-mails into tickets"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "jschreuder\\SpotDesk\\": "src"
        }
    }
}
